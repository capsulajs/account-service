{
  "presets": ["@babel/preset-flow", "@babel/preset-env"],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "commonjs": {
      "plugins": [
        ["@babel/plugin-transform-modules-commonjs", {"loose" :true }]
      ]
    },
    "jest": {
      "plugins": [
        ["@babel/plugin-transform-modules-commonjs", {"loose" :true }]
      ]
    }
  }
}
