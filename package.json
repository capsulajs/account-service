{
  "name": "account-service",
  "description": "Account service",
  "version": "0.0.1",
  "main": "src/AccountService.js",
  "scripts": {
    "rm:lib": "rimraf lib",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build": "yarn rm:lib && yarn build:commonjs",
    "test": "cross-env BABEL_ENV=jest jest",
    "jsdoc": "jsdoc -r src/ -c jsdoc_conf.js -d ./docs"
  },
  "jest": {
    "testRegex": ".*\\.test\\.js$",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capsulajs/account-service.git"
  },
  "keywords": [
    "microfrontend",
    "service",
    "account",
    "login",
    "authentication"
  ],
  "author": "Stephane Benayoun (https://github.com/stephanebenayoun)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capsulajs/account-service/issues"
  },
  "homepage": "https://github.com/capsulajs/account-service#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "flow": "^0.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "flow-bin": "^0.80.0",
    "jest": "^23.6.0",
    "jsdoc": "~3.5.5",
    "jsdoc-babel": "^0.5.0",
    "ws": "^6.0.0"
  }
}
